{
    "collectCoverageFrom": [
        "src/components/**/*.{js,jsx,ts,tsx}",
        "!src/components/**/*.{types,stories,constants,test,spec}.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
