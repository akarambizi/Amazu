{
    "name": "a-rent",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/axios": "^0.14.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.11",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "axios": "^1.4.0",
        "eslint": "^8.49.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-query": "^3.39.3",
        "react-scripts": "5.0.1",
        "styled-components": "^5.3.6",
        "typescript": "^5.2.2",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "server": "cd ../server && yarn serve",
        "mock-server": "json-server --watch src/mock-server/index.js --port 4200",
        "dev": "REACT_APP_API_ENV=dev concurrently \"yarn start\" \"yarn server\"",
        "dev:mock": "REACT_APP_API_ENV=mock concurrently \"yarn start\" \"yarn mock-server\"",
        "build": "react-scripts build",
        "test": "react-scripts test --coverage --watchAll --detectOpenHandles",
        "eject": "react-scripts eject",
        "lint": "eslint ./ --ext js,jsx,ts,tsx --ignore-path .gitignore",
        "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.tsx' --write",
        "prepare": "cd .. && husky install .husky | chmod ug+x .husky/*",
        "precommit": "lint-staged"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "concurrently": "^8.2.2",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.3",
        "json-server": "^0.17.1",
        "lint-staged": "^13.1.0",
        "nodemon": "^3.0.1",
        "prettier": "^2.8.3",
        "prettier-eslint": "^15.0.1",
        "prettier-eslint-cli": "^7.1.0",
        "react-router-dom": "^6.15.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,md}": [
            "prettier --write",
            "eslint --fix --ext .js,.jsx,.ts,.tsx"
        ]
    }
}
